{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": [
        "static/js/*"
    ],
    "rules": {
        "prettier/prettier": "error",
        "linebreak-style": ["error", "unix"],
        "no-console": "off"
    }
}
